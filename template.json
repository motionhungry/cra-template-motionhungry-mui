{
  "package": {
    "dependencies": {
      "@craco/craco": "^5.8.0",
      "@material-ui/core": "^4.11.0",
      "@material-ui/icons": "^4.9.1",
      "@material-ui/lab": "^4.0.0-alpha.56",
      "@material-ui/utils": "^4.9.12",
      "@sentry/react": "^5.24.2",
      "@sentry/tracing": "^5.24.2",
      "@storybook/addon-actions": "^6.0.21",
      "@storybook/addon-knobs": "^6.0.21",
      "@storybook/addon-links": "^6.0.21",
      "@storybook/addons": "^6.0.21",
      "@storybook/preset-create-react-app": "^3.1.4",
      "@storybook/react": "^6.0.21",
      "@testing-library/jest-dom": "^5.11.4",
      "@testing-library/react": "^11.1.0",
      "@testing-library/user-event": "^12.1.10",
      "@types/jest": "^26.0.15",
      "@types/node": "^12.0.0",
      "@types/react": "^16.9.53",
      "@types/react-dom": "^16.9.8",
      "@types/react-router-dom": "^5.1.6",
      "craco-alias": "^2.1.1",
      "eslint-plugin-jsx-a11y": "^6.3.1",
      "eslint-config-prettier": "^6.15.0",
      "eslint-plugin-prettier": "^3.1.4",
      "eslint-plugin-react": "^7.20.6",
      "eslint-plugin-react-hooks": "^4.1.2",
      "prettier": "^2.2.0",
      "react": "^17.0.1",
      "react-dom": "^17.0.1",
      "react-router-dom": "^5.2.0",
      "react-scripts": "4.0.1",
      "typescript": "^4.0.3",
      "source-map-explorer": "^2.5.0",
      "web-vitals": "^0.2.4"
    },
    "scripts": {
      "analyze": "source-map-explorer 'build/static/js/*.js'",
      "start": "REACT_APP_VERSION=local craco start",
      "build:staging": "REACT_APP_VERSION=$npm_package_version@$(git rev-parse --short HEAD) craco build",
      "build:prod": "REACT_APP_VERSION=$npm_package_version craco build",
      "test": "REACT_APP_VERSION=$npm_package_version craco test",
      "test:nowatch": "REACT_APP_VERSION=$npm_package_version craco test --watchAll=false",
      "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
      "lint": "eslint src/",
      "storybook": "start-storybook -p 9009 -s public",
      "build-storybook": "build-storybook -s public"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ]
    }
  }
}
