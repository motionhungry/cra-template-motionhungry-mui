    "scripts": {
      "analyze": "source-map-explorer 'build/static/js/*.js'",
      "start": "REACT_APP_VERSION=local react-app-rewired start",
      "build:staging": "REACT_APP_VERSION=$npm_package_version@$(git rev-parse --short HEAD) react-app-rewired build",
      "build:prod": "REACT_APP_VERSION=$npm_package_version react-app-rewired build",
      "test": "REACT_APP_VERSION=$npm_package_version react-app-rewired test",
      "test:nowatch": "REACT_APP_VERSION=$npm_package_version react-app-rewired test --watchAll=false",
      "lint:fix": "eslint --fix",
      "lint": "eslint src/",
      "storybook": "start-storybook -p 9009 -s public",
      "build-storybook": "build-storybook -s public"
    }
  }
